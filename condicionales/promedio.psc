Proceso promedio
	Definir a,b,c Como real;
	Escribir "Ingresa la primera nota";
	leer a;
	Escribir "Ingresar la segunda nota";
	Leer b;
	Escribir "Ingresar la tercera nota";
	leer c;
	c<-a+b/2;
	Si c>= 8 Entonces
		Escribir "Aprobado";
	SiNo
		Escribir"Reprobado";
	FinSi
	
	
	
FinProceso
